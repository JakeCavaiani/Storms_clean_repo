names(moos.fdom.final) <- c("DateTime", "fDOM.QSU") 

MOOS.SpCond = subset(MOOS.2018, select = c("DateTime","SpCond.uScm"))
MOOS.SpCond$SpCond.uScm <- as.numeric(MOOS.SpCond$SpCond.uScm)
