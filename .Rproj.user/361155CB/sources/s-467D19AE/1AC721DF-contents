     xlim = as.POSIXct(c("2021-05-01 0:00:00","2021-10-01 00:00:00"), tz="America/Anchorage"),
     ylim = c(7,0), 
     axes=F, xlab="", ylab="")
axis(side = 4)
par(mfrow=c(1,1))
