moos.no3.final$DateTime <-as.POSIXct(moos.no3.final$moos.no3.1, "%Y-%m-%d %H:%M:%S", tz="America/Anchorage")

MOOS.fDOM = subset(MOOS.2018, select = c("DateTime","fDOM.QSU"))
