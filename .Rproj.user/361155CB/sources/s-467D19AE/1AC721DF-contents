     xlim = as.POSIXct(c("2019-04-29 00:00:00","2019-05-31 23:45:00"), tz="America/Anchorage"))
abline(h=CARI_bfQ_mn*2, col="red", lty=2)
abline(h=CARI_bfQ_mn, col="red")
par(new = T)

