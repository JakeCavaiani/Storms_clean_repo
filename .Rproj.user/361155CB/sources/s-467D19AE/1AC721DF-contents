FRCH_storm1_06_13 = FRCH[FRCH$DateTime > as.POSIXct("2020-06-13 03:30:00", tz="America/Anchorage") &
                           FRCH$DateTime < as.POSIXct("2020-06-18 05:30:00", tz="America/Anchorage"),]
