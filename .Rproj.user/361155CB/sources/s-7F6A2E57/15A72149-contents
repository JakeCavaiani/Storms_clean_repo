  VAUL_turb_storm_list[[i]][["Turbidity.FNU"]] = NULL
  VAUL_turb_storm_list[[i]][["fDOM.QSU.mn"]] = NULL
  VAUL_turb_storm_list[[i]][["SpCond.S.cm"]] = NULL
