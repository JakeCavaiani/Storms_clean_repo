               parse = TRUE)
FI_beta_comp_SPC

FI_beta_comp_turb = 
  ggplot(HI_FI_turb, aes(Flush_index, beta)) + geom_point(aes(colour=factor(site.ID)), size = 4) +
  ylim(-1.5, 1.5) + xlim(-1.5, 1.5) +
