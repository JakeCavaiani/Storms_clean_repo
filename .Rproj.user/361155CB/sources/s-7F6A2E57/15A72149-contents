                             rep("storm5", 239),
                             rep("storm6", 123),
                             rep("storm7a", 99),
                             rep("storm7b", 96),
                             rep("storm8", 83),
                             rep("storm9a", 295),
                             rep("storm9b", 135),
                             rep("storm9c", 483))

