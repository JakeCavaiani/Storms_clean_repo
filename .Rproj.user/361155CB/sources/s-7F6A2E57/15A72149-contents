HI.cari.2018 <- rbind(HI.cari.no3.2.2018, HI.cari.fDOM.2.2018, HI.cari.SPC.2.2018, HI.cari.turb.2.2018) # merging all responses together 
HI.cari.2018$burn <- "burned" # adding a burn column
