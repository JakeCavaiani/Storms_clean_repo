  FRCH_fDOM_storm_list[[i]][["fDOM.QSU.mn"]] = NULL
  FRCH_fDOM_storm_list[[i]][["SpCond.S.cm"]] = NULL
  FRCH_fDOM_storm_list[[i]][["Turbidity.FNU"]] = NULL
