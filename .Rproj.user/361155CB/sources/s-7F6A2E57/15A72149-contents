  MOOS_fDOM_storm_list[[i]][["fDOM.QSU.mn"]] = NULL
  MOOS_fDOM_storm_list[[i]][["SpCond.S.cm"]] = NULL
  MOOS_fDOM_storm_list[[i]][["Turbidity.FNU"]] = NULL
