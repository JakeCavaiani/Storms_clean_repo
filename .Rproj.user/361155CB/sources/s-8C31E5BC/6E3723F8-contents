               label.y = "top", label.x = "right",
               aes(label = paste(..eq.label.., ..rr.label.., sep = "~~~")), 
               parse = TRUE) + 
  ylab("Hysteresis Index") +
  ggtitle("HI vs. ThreeMonth")

