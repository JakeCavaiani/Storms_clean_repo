  geom_errorbar(aes(ymin=FI_ymin, ymax=FI_ymax), colour="black", alpha=0.5, size=.5, width = 0.1)+ 
  geom_point(aes(color = site.ID), size = 2) +
  scale_color_manual(values = c("orange red", viridis::viridis(6)),
