                        rep("CARI_storm11_09_02_fDOM", 50),
                        rep("CARI_storm11_09_02_SPC", 50),
                        rep("CARI_storm11_09_02_turb", 50),
                        rep("CARI_storm12a_09_20_NO3", 50),
