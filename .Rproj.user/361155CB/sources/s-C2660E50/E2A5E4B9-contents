```{r}
### TKH report ###
# Load in 2018 data # 
FRCH_HI_doy_df_2018 <- read_csv("~/Documents/Storms/Output_from_analysis/HI_plots/2018/FRCH/FRCH.HI.doy.df.csv")
MOOS_HI_doy_df_2018 <- read_csv("~/Documents/Storms/Output_from_analysis/HI_plots/2018/MOOS/MOOS.HI.doy.df.csv")
# Load in 2019 data #
POKE_HI_df_doy_2019 <- read_csv("~/Documents/DoD_2019/Storm Analysis/Output_from_analysis/POKE.HI.df.doy.2019.csv")

VAUL_HI_df_doy_2019 <- read_csv("~/Documents/DoD_2019/Storm Analysis/Output_from_analysis/VAUL.HI.df.doy.2019.csv")

MOOS_HI_df_doy_2019 <- read_csv("~/Documents/DoD_2019/Storm Analysis/Output_from_analysis/MOOS.HI.df.doy.2019.csv")

FRCH_HI_df_doy_2019 <- read_csv("~/Documents/DoD_2019/Storm Analysis/Output_from_analysis/FRCH.HI.df.doy.2019.csv")

STRT_HI_df_doy_2019 <- read_csv("~/Documents/DoD_2019/Storm Analysis/Output_from_analysis/STRT.HI.df.doy.2019.csv")

MOOS_HI_df <- read_csv("~/Documents/DoD_Discharge/Output for analysis/MOOS.HI.df.csv")

FRCH_HI_df <- read_csv("~/Documents/DoD_Discharge/Output for analysis/FRCH.HI.df.csv")

FRCH_HI_df$date <- as.Date(with(FRCH_HI_df, paste(month, day, sep = "-")), "%m-%d")
FRCH_HI_df$doy <- yday(FRCH_HI_df$date)

STRT_HI_doy_df <- read_csv("Output for analysis/STRT.HI.doy.df.csv")

STRT_HI_df$date <- as.Date(with(STRT_HI_df, paste(month, day, sep = "-")), "%m-%d")
STRT_HI_df$doy <- yday(STRT_HI_df$date) 

POKE_HI_df <- read_csv("~/Documents/DoD_Discharge/Output for analysis/POKE.HI.df.csv")

VAUL_HI_doy_df <- read_csv("Output for analysis/VAUL.HI.doy.df.csv")

FRCH_HI_doy_df_2018$year <- "2018"
MOOS_HI_doy_df_2018$year <- "2018"

POKE_HI_df_doy_2019$year <- "2019"
VAUL_HI_df_doy_2019$year <- "2019"
FRCH_HI_df_doy_2019$year <- "2019"
MOOS_HI_df_doy_2019$year <- "2019"
STRT_HI_df_doy_2019$year <- "2019"

POKE_HI_df$year <- "2020"
VAUL_HI_doy_df$year <- "2020"
FRCH_HI_df$year <- "2020"
MOOS_HI_df$year <- "2020"
STRT_HI_doy_df$year <- "2020"

comb.2019 <- rbind(POKE_HI_df_doy_2019, VAUL_HI_df_doy_2019, FRCH_HI_df_doy_2019, MOOS_HI_df_doy_2019, STRT_HI_df_doy_2019)
comb.2020 <- rbind(POKE_HI_df, VAUL_HI_doy_df, FRCH_HI_df, MOOS_HI_df, STRT_HI_doy_df)
comb.all <- rbind(comb.2019, comb.2020)

g0 <- comb.all %>% filter(response == "NO3") %>%
  ggplot(aes(x = doy, y = HI, label = doy, fill=response))
g1 = g0 + geom_jitter(width = 0.1, fill = "grey", colour = "#0571B0", alpha=0, size=3) + 
  theme(axis.text.x = element_text(angle = 0))+  labs(x="") + facet_wrap(~ response, scales = "free_x") +
theme_bw() +geom_hline(yintercept=0) + theme(axis.text.x = element_text(angle = 90, hjust = 1))+ theme(legend.position = "none")
 
g2 <- g1 + 
  stat_summary(fun.data = median_cl_boot, geom = "errorbar",
               colour = "black", width = 0.2, size=1) + 
  stat_summary(fun.y = median, geom = "point", 
               colour = "black", shape = year, size = 3)
g2
ggsave("/Users/jakecavaiani/Documents/DoD_Discharge/Output for analysis/all.sites.median.boot.pdf", plot=g2, width = 8, height = 6, units ="in")




comb.all.no3 <- comb.all %>% filter(response == "NO3") %>%
  ggplot(aes(x = doy, y = HI))
comb.all.no3


med.POKE.HI <- POKE.HI.df %>% group_by(storm.ID) %>%
                summarize(HI_med = median(HI))
  
g0 <- ggplot(comb.HI.df, aes(x = storm.num, y = HI, label=storm.num, fill=response))
g1 = g0 + geom_jitter(width = 0.1, fill = "grey", colour = "#0571B0", alpha=0.25, size=3) + 
  theme(axis.text.x = element_text(angle = 0))+  labs(x="") + facet_wrap(~ response, scales = "free_x") +
  theme_bw() +geom_hline(yintercept=0) + theme(axis.text.x = element_text(angle = 90, hjust = 1)) + theme(legend.position = "none")
g2 <- g1 + 
  stat_summary(fun.data = median_cl_boot, geom = "errorbar",
               colour = site.ID, width = 0.2, size=1) + 
  stat_summary(fun.y = median, geom = "point", 
               colour = Site, shape = year, size = 3)

comb.all.no3 <- comb.all %>% filter(response == "NO3")
g0 <- ggplot(comb.all.no3, aes(x = doy, y = HI, fill = site.ID, shape = year))
g1 = g0 + geom_jitter(width = 0.1, fill = "grey", colour = "#0571B0", alpha=0, size=3) + 
  theme(axis.text.x = element_text(angle = 0))+  labs(x="") + facet_wrap(~ response, scales = "free_x") +
  theme_bw() +geom_hline(yintercept=0) + theme(axis.text.x = element_text(angle = 90, hjust = 1)) + theme(legend.position = "none")
g2 <- g1 + 
  stat_summary(fun.data = median_cl_boot, geom = "errorbar",
               colour = "black", width = 0.2, size=1) + 
  stat_summary(fun.y = median, geom = "point", 
               colour = "black", size = 3)
ggsave("/Users/jakecavaiani/Documents/DoD_Discharge/Output for analysis/POKE.HI.median.boot.doy.pdf", plot=g2, width = 8, height = 6, units ="in")
write.csv(POKE.HI.df, "/Users/jakecavaiani/Documents/DoD_Discharge/Output for analysis/POKE.HI.df.csv")

comb.all.fDOM <- comb.all %>% filter(response == "fDOM")
g0 <- ggplot(comb.all.fDOM, aes(x = doy, y = HI, shape = site.ID))
g1 = g0 + geom_jitter(width = 0.1, fill = "grey", colour = "#0571B0", alpha=0, size=3) + 
  theme(axis.text.x = element_text(angle = 0))+  labs(x="") + facet_wrap(~ response, scales = "free_x") +
  theme_bw() +geom_hline(yintercept=0) + theme(axis.text.x = element_text(angle = 90, hjust = 1)) + theme(legend.position = "none")
g2 <- g1 + 
  stat_summary(fun.data = median_cl_boot, geom = "errorbar",
               colour = "black", width = 0.2, size=1) + 
  stat_summary(fun.y = median, geom = "point",
               colour = "black", size = 3)
g3 <- g2 + scale_color_manual(values=c("#999999", "#E69F00"))
ggsave("/Users/jakecavaiani/Documents/DoD_Discharge/Output for analysis/POKE.HI.median.boot.doy.pdf", plot=g2, width = 8, height = 6, units ="in")
write.csv(POKE.HI.df, "/Users/jakecavaiani/Documents/DoD_Discharge/Output for analysis/POKE.HI.df.csv")

write.csv(comb.all, "/Users/jakecavaiani/Documents/DoD_Discharge/Output for analysis/comb.all.HI.df.csv")

