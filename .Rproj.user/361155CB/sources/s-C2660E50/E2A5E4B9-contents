FRCH.hyst.results.list = list(py$hysdict_FRCH_storm1_07_23_NO3,
                              py$hysdict_FRCH_storm2_07_27_NO3,
                              py$hysdict_FRCH_storm2_07_27_fDOM,
                              py$hysdict_FRCH_storm2_07_27_SPC,
                              py$hysdict_FRCH_storm2_07_27_turb,
