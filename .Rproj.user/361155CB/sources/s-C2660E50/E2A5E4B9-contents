```{python}
# MOOS #
MOOS_storm1_06_20_Q = pd.read_csv("~/Documents/Storms/Storm_Events/2020/MOOS/MOOS_storm1_06_20_Q.csv")
MOOS_storm1_06_20_NO3 = pd.read_csv("~/Documents/Storms/Storm_Events/2020/MOOS/MOOS_storm1_06_20_NO3.csv")
MOOS_storm1_06_20_fDOM =pd.read_csv("~/Documents/Storms/Storm_Events/2020/MOOS/MOOS_storm1_06_20_fDOM.csv")
MOOS_storm1_06_20_SPC =pd.read_csv("~/Documents/Storms/Storm_Events/2020/MOOS/MOOS_storm1_06_20_SPC.csv")
MOOS_storm1_06_20_turb =pd.read_csv("~/Documents/Storms/Storm_Events/2020/MOOS/MOOS_storm1_06_20_Turb.csv")

