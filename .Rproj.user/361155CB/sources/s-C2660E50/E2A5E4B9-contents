FRCH_storm10c_08_03_Q['valuedatetime'] =  pd.to_datetime(FRCH_storm10c_08_03_Q['valuedatetime'], format='%Y-%m-%d %H:%M:%S')
FRCH_storm10c_08_03_turb['valuedatetime'] =  pd.to_datetime(FRCH_storm10c_08_03_turb['valuedatetime'], format='%Y-%m-%d %H:%M:%S')
timespacing = 15 # 15 minutes between records
hysdict_FRCH_storm10c_08_03_turb = hysteresisMetrics(FRCH_storm10c_08_03_Q, FRCH_storm10c_08_03_turb, timespacing, timespacing, debug=False, interpall=True, discharge_time_spacing_units='minutes', response_time_spacing_units='minutes', discharge_units='Lsec')

