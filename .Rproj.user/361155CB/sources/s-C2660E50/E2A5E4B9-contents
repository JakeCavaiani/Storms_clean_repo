MOOS_storm6b_08_02_SPC['valuedatetime'] =  pd.to_datetime(MOOS_storm6b_08_02_SPC['valuedatetime'], format='%Y-%m-%d %H:%M:%S')
timespacing = 15 # 15 minutes between records
