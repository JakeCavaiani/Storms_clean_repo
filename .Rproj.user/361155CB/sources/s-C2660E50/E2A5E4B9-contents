                        rep("CARI_storm9_08_26_turb", 50),
                        rep("CARI_storm10_08_30_NO3", 50),
                        rep("CARI_storm10_08_30_fDOM", 50),
                        rep("CARI_storm10_08_30_SPC", 50),
                        rep("CARI_storm10_08_30_turb", 50),
